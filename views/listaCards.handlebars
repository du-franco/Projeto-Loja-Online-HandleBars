<div class="container">
    <div class="row">
        {{#each resultado}}
        <div class="col-lg-3 col-md-6 col-sm-12">
            <div class="card" style="width: 17rem;">
                <img src="img/{{foto}}" class="card-img-top">
                <div class="card-body">
                    <h5 class="card-title">{{nome}}</h5>
                    <p class="card-text">R${{valor}}</p>
                    <a href="/inserirCarrinho/{{_id}}" class="btn btn-primary">Comprar</a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>